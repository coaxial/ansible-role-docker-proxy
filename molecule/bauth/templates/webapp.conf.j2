server {
  listen 80;

  location /webapp/ {
    auth_basic "Restricted";
    auth_basic_user_file /etc/nginx/.htpasswd;
    proxy_pass http://{{ _dp__docker_gw_ip.stdout }}:{{ upstream.port }};
  }

  location /nope/ {
    auth_basic "Restricted";
    auth_basic_user_file /etc/nginx/.htpasswd;
    alias /usr/share/nginx/html/;
    index index.html index.htm;
  }
}
